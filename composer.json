{
  "name": "drupal-composer/drupal-project",
  "description": "Project template for Drupal 9 projects with a relocated document root",
  "type": "project",
  "license": "GPL-3.0-or-later",
  "homepage": "https://www.drupal.org/project/drupal",
  "support": {
    "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
    "chat": "https://www.drupal.org/node/314178"
  },
  "authors": [
    {
      "name": "IC Webteam",
      "email": "webteam@gatech.edu",
      "role": "Makers of things like this"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "path",
      "url": "gt/gt_profile"
    },
    {
      "type": "path",
      "url": "gt/gt_theme"
    },
    {
      "type": "path",
      "url": "gt/gt_tools"
    },
    {
      "type": "path",
      "url": "gt/hg_reader"
    },
    {
      "type": "path",
      "url": "gt/gt_editor"
    }
  ],
  "require": {
    "php": ">=8.1",
    "composer/installers": "^1.9",
    "drupal/admin_toolbar": "^3.0",
    "drupal/back_to_top": "^3.0",
    "drupal/better_exposed_filters": "^6.0",
    "drupal/block_region_permissions": "^1.4",
    "drupal/cas": "^2.0",
    "drupal/ckeditor": "^1.0",
    "drupal/classy": "^1.0",
    "drupal/colorbox": "^2.0",
    "drupal/core-composer-scaffold": "^10.0",
    "drupal/core-project-message": "^10.0",
    "drupal/core-recommended": "^10.0",
    "drupal/custom_add_another": "^2.0@beta",
    "drupal/devel": "^5.0",
    "drupal/editor_advanced_link": "^2.2",
    "drupal/editor_file": "^1.6",
    "drupal/entity_reference_revisions": "^1.9",
    "drupal/externalauth": "^2.0",
    "drupal/faqfield": "^7.0",
    "drupal/field_formatter_class": "^1.5",
    "drupal/field_group": "^3.1",
    "drupal/focal_point": "^2.0",
    "drupal/google_analytics": "^4.0",
    "drupal/google_tag": "^1.5",
    "drupal/imce": "^3.0",
    "drupal/inline_entity_form": "^1.0@RC",
    "drupal/jquery_ui_accordion": "^2.0",
    "drupal/jquery_ui_datepicker": "^2.0",
    "drupal/jquery_ui_slider": "^2.0",
    "drupal/layout_builder_modal": "^1.1",
    "drupal/layout_builder_restrictions": "^2.7",
    "drupal/masquerade": "^2.0@beta",
    "drupal/metatag": "^1.19",
    "drupal/module_filter": "^4.0",
    "drupal/paragraphs": "^1.12",
    "drupal/pathauto": "^1.10",
    "drupal/pathologic": "^2.0@alpha",
    "drupal/quickedit": "^1.0",
    "drupal/rdf": "^2.0",
    "drupal/redirect": "^1.6",
    "drupal/search404": "^2.0",
    "drupal/simple_sitemap": "^4.0",
    "drupal/social_media_links": "^2.8",
    "drupal/stable": "^2.0",
    "drupal/token": "^1.9",
    "drupal/upgrade_status": "^4.0",
    "drupal/video_embed_field": "^2.4",
    "drupal/view_unpublished": "^1.0",
    "drupal/views_accordion": "^2.0",
    "drupal/xmlsitemap": "^1.2",
    "drush/drush": "^11.0",
    "gt/gt_editor": "*",
    "gt/gt_profile": "*",
    "gt/gt_theme": "*",
    "gt/gt_tools": "*",
    "gt/hg_reader": "*",
    "platformsh/config-reader": "^2.4",
    "webflo/drupal-finder": "^1.0.0",
    "webmozart/path-util": "^2.3"
  },
  "conflict": {
      "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
      "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "drupal/core-composer-scaffold": true,
      "drupal/core-project-message": true
    }
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "web"
      },
      "file-mapping": {
        "[web-root]/.htaccess": false
      }
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "web/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "web/profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "web/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "drupal-core-project-message": {
      "include-keys": [
        "homepage",
        "support"
      ]
    }
  }
}
