{#
/**
 * @file gt_help_topic_entity.html.twig
 * Default theme implementation to present GT Help Topic data.
 *
 * This template is used when viewing GT Help Topic pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_gt_help_topic_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('gt_help_topic_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
